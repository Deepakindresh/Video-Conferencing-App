# Set the domain (change if hosting on a real server)
DOMAIN=localhost

# WebSocket Server (Protoo) Port
PROTOO_LISTEN_PORT=4443

# TLS certificate paths
HTTPS_CERT_FULLCHAIN=~/mediasoup/certs/fullchain.pem
HTTPS_CERT_PRIVKEY=~/mediasoup/certs/privkey.pem

# Mediasoup Worker DTLS certificates (if applicable)
WORKER_CERT_FULLCHAIN=~/mediasoup/certs/fullchain.pem
WORKER_CERT_PRIVKEY=~/mediasoup/certs/privkey.pem

# Mediasoup Server Configuration
MEDIASOUP_LISTEN_IP=0.0.0.0
MEDIASOUP_ANNOUNCED_IP=127.0.0.1

# WebRTC Transport Port Range
MEDIASOUP_MIN_PORT=40000
MEDIASOUP_MAX_PORT=49999
